{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@now/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      },
      {
        "src": "/(.+)",
        "dest": "public/$1"
      }
    ]
  }